<?xml version="1.0" encoding="UTF-8"?>
<jmeterTestPlan version="1.2" properties="5.0" jmeter="5.6.3">
  <hashTree>
    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="AI Assistant Performance Test">
      <stringProp name="TestPlan.comments">Dynamic properties - pass via -J flags at runtime</stringProp>
