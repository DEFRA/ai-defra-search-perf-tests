          <hashTree/>
          <RegexExtractor guiclass="RegexExtractorGui" testclass="RegexExtractor" testname="Extract Conversation ID" enabled="true">
            <stringProp name="RegexExtractor.useHeaders">URL</stringProp>
            <stringProp name="RegexExtractor.refname">conversationId</stringProp>
            <stringProp name="RegexExtractor.regex">/start/([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})</stringProp>
            <stringProp name="RegexExtractor.template">$1$</stringProp>
            <stringProp name="RegexExtractor.default">CONVERSATION_ID_NOT_FOUND</stringProp>
            <stringProp name="RegexExtractor.match_number">1</stringProp>
          </RegexExtractor>
          <hashTree/>
          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Debug - Log Response Body" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">
log.info("=== QUESTION 1 RESPONSE DEBUG ===")
log.info("Response Code: " + prev.getResponseCode())
log.info("Response Body (first 5000 chars): " + prev.getResponseDataAsString().take(5000))
log.info("Extracted conversationId: " + vars.get("conversationId"))
log.info("=== END DEBUG ===")
